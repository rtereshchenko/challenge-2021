{
  "name": "challenge-2021",
  "version": "1.0.0",
  "description": "ETL worker for csv files",
  "main": "index.js",
  "scripts": {
    "postinstall": "husky install",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "clear": "rm -rf node_modules",
    "test": "MONGO_URI=mongodb://localhost:27017 MONGO_DB=db MOCHA_FILE=./output/report.xml mocha --recursive --timeout 5000 --reporter-options outputs=true --exit tests",
    "test-ci": "MONGO_URI=mongodb://localhost:27017 MONGO_DB=db MOCHA_FILE=./output/report.xml mocha --recursive --reporter mocha-junit-reporter --timeout 5000 --reporter-options outputs=true --exit tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/russellswift/challenge-2021.git"
  },
  "keywords": [
    "ETL",
    "worker",
    "csv",
    "mongo"
  ],
  "author": "Ruslan Tereshchenko <mr.russell.swift@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/russellswift/challenge-2021/issues"
  },
  "homepage": "https://github.com/russellswift/challenge-2021#readme",
  "devDependencies": {
    "chai": "^4.3.4",
    "eslint": "^7.22.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "husky": "^5.2.0",
    "mocha": "^8.3.2",
    "mocha-junit-reporter": "^2.0.0"
  },
  "dependencies": {
    "csvtojson": "^2.0.10",
    "mongodb": "^3.6.5",
    "uuid": "^8.3.2"
  }
}
